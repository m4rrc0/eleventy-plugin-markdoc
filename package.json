{
  "name": "@m4rrc0/eleventy-plugin-markdoc",
  "version": "0.1.7",
  "description": "An Eleventy plugin to add support for Markdoc as template language",
  "type": "module",
  "homepage": "https://github.com/m4rrc0/eleventy-plugin-markdoc",
  "main": "eleventy.config.js",
  "scripts": {
    "start": "eleventy --serve",
    "build": "eleventy",
    "clean": "rm -rf _site",
    "bump": "npm --no-git-tag-version version",
    "patch": "npm version patch",
    "preversion": "npm run build",
    "version": "git add -A",
    "postversion": "git push && git push --tags && npm run clean",
    "publish": "npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m4rrc0/eleventy-plugin-markdoc.git"
  },
  "keywords": [
    "11ty",
    "11ty-plugin",
    "eleventy",
    "eleventy-plugin",
    "markdoc"
  ],
  "author": {
    "name": "m4rrc0",
    "url": "https://github.com/m4rrc0"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/m4rrc0/eleventy-plugin-markdoc/issues"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@markdoc/markdoc": "^0.5.1",
    "fast-glob": "^3.3.3"
  },
  "peerDependencies": {
    "@11ty/eleventy": "^3.0.0"
  }
}
